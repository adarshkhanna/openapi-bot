Platform OpenAPIPlatform

path "com.xatkit.plugins.openapi.platform.OpenAPIPlatform"

actions {
	LoadAPI("url")
	//path
	ListPaths
	PathDetails(relativePath)
	// pathIdentifier = Integer for now
	ListOperationsOnPathUsingRowNumber(pathIdentifier)
	GetLicense
	//Security
	SupportedSchemes
	IsAuthenticationRequired
	IsAuthenticationRequiredOperation(operationId)
	SecuritySchemeList
	SecuritySchemeDetails(securitySchemeId)
}