<?xml version="1.0" encoding="ASCII"?>
<intent:Library xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:intent="com.xatkit.intent" name="OpenAPIExampleLibrary">
  <eventDefinitions xsi:type="intent:IntentDefinition" name="ApiUrl">
    <outContexts name="API">
      <parameters name="url" textFragment="URL">
        <entity xsi:type="intent:BaseEntityDefinitionReference">
          <baseEntity entityType="url"/>
        </entity>
      </parameters>
    </outContexts>
    <trainingSentences>URL</trainingSentences>
  </eventDefinitions>
  <eventDefinitions xsi:type="intent:IntentDefinition" name="ListPaths" followedBy="//@eventDefinitions.2">
    <trainingSentences>Show me the list of paths in the API</trainingSentences>
    <trainingSentences>Show me the list of paths</trainingSentences>
    <trainingSentences>Display the relative paths</trainingSentences>
    <trainingSentences>What are the paths?</trainingSentences>
  </eventDefinitions>
  <eventDefinitions xsi:type="intent:IntentDefinition" name="ListOperationsOnPathUsingRowNumber" follows="//@eventDefinitions.1">
    <outContexts name="Path">
      <parameters name="pathIdentifier" textFragment="XXX">
        <entity xsi:type="intent:BaseEntityDefinitionReference">
          <baseEntity entityType="number"/>
        </entity>
      </parameters>
    </outContexts>
    <trainingSentences>List the operations of the path on the position XXX</trainingSentences>
  </eventDefinitions>
  <eventDefinitions xsi:type="intent:IntentDefinition" name="PathDetails">
    <outContexts name="RelativePath">
      <parameters name="relativePath" textFragment="XXX">
        <entity xsi:type="intent:BaseEntityDefinitionReference">
          <baseEntity/>
        </entity>
      </parameters>
    </outContexts>
    <trainingSentences>Tell me more about the path xxx</trainingSentences>
    <trainingSentences>Tell me more about the path with has the relative path XXX</trainingSentences>
  </eventDefinitions>
  <eventDefinitions xsi:type="intent:IntentDefinition" name="GetNumberOfPath">
    <trainingSentences>Give me the number of paths</trainingSentences>
    <trainingSentences>How many paths are there in the API</trainingSentences>
    <trainingSentences>How many paths?</trainingSentences>
  </eventDefinitions>
  <eventDefinitions xsi:type="intent:IntentDefinition" name="AskLicenseInfo">
    <trainingSentences>What is the license associated with this API?</trainingSentences>
  </eventDefinitions>
  <eventDefinitions xsi:type="intent:IntentDefinition" name="SupportedSchemes">
    <trainingSentences>What are the transfer protocols supported by this API?</trainingSentences>
  </eventDefinitions>
  <eventDefinitions xsi:type="intent:IntentDefinition" name="IsAuthenticationRequired">
    <trainingSentences>Does this API require authentication?</trainingSentences>
  </eventDefinitions>
  <eventDefinitions xsi:type="intent:IntentDefinition" name="IsAuthenticationRequiredOperation">
    <outContexts name="Operation">
      <parameters name="operationId" textFragment="xxx">
        <entity xsi:type="intent:BaseEntityDefinitionReference">
          <baseEntity/>
        </entity>
      </parameters>
    </outContexts>
    <trainingSentences>Does the operation xxx require authentication?</trainingSentences>
  </eventDefinitions>
  <eventDefinitions xsi:type="intent:IntentDefinition" name="SecuritySchemeList">
    <trainingSentences>What are the authentication methods provided by this API?</trainingSentences>
    <trainingSentences>What are the authentication methods declared by this API?</trainingSentences>
    <trainingSentences>What are the security schemes provided by this API?</trainingSentences>
  </eventDefinitions>
  <eventDefinitions xsi:type="intent:IntentDefinition" name="SecuritySchemeDetails">
    <outContexts name="SecurityScheme">
      <parameters name="securitySchemeId" textFragment="xxx">
        <entity xsi:type="intent:BaseEntityDefinitionReference">
          <baseEntity/>
        </entity>
      </parameters>
    </outContexts>
    <trainingSentences>Tell me more about the security scheme xxx</trainingSentences>
    <trainingSentences>Tell me more about the authentication method xxx</trainingSentences>
  </eventDefinitions>
</intent:Library>
